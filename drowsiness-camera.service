[Unit]
Description=Drowsiness Detection Camera Stream
After=network.target drowsiness-backend.service

[Service]
Type=simple
User=entung
WorkingDirectory=/home/entung/deteksikantuk
Environment="PATH=/home/entung/deteksikantuk/venv/bin"
ExecStart=/home/entung/deteksikantuk/venv/bin/python camera_stream.py
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target
